{
    "name": "causal/csl_oauth2",
    "type": "typo3-cms-extension",
    "description": "This extension transforms your TYPO3 website into an OAuth2 server.",
    "keywords": [
        "TYPO3",
        "CMS",
        "OAuth2",
        "Security",
        "Web Service"
    ],
    "homepage": "https://github.com/portrino/csl_oauth2",
    "support": {
        "issues": "https://github.com/portrino/csl_oauth2/issues"
    },
    "authors": [
        {
            "name": "Xavier Perseguers",
            "email": "xavier@causal.ch",
            "homepage": "https://www.causal.ch",
            "role": "Developer"
        },
        {
            "name": "Axel Boeswetter",
            "email": "boeswetter@portrino.de",
            "homepage": "http://www.portrino.de",
            "role": "Developer"
        }
    ],
    "license": "GPL-2.0+",
    "require": {
        "php": ">= 5.5.0, <= 7.2.99",
        "typo3/cms-core": ">= 6.2.0, <= 8.99.99",
        "bshaffer/oauth2-server-php": "~1.9.0"
    },
    "autoload": {
        "psr-4": {
            "Causal\\CslOauth2\\": "Classes/"
        }
    },
    "replace": {
        "csl_oauth2": "self.version",
        "typo3-ter/csl-oauth2": "self.version"
    }
}